datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Post {
  body      String
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  slug      String
  title     String
}
